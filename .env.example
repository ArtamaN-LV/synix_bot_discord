# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Bot Client ID
CLIENT_ID=your_client_id_here

# Server (Guild) ID
GUILD_ID=your_guild_id_here

# MongoDB Connection String
MONGODB_URI=your_mongodb_connection_string_here

# Optional: Bot Owner ID (for owner-only commands)
BOT_OWNER_ID=your_discord_user_id_here

# Optional: Default Prefix (if you want to support message commands)
DEFAULT_PREFIX=!

# Optional: Logging Level (info, debug, error)
LOG_LEVEL=info

# Stats Channel ID
STATS_CHANNEL_ID=your_stats_channel_id_here

# Ticket System Configuration
TICKET_CATEGORY_ID=your_ticket_category_id_here
SUPPORT_ROLE_ID=your_support_role_id_here
MEMBERS_CHANNEL_ID=your_members_channel_id_here 